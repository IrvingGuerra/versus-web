  "description": "Auth Scaffold with Meteor and Vue",
  "author": "Antware",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "run:mac": "MONGO_URL=mongodb://localhost:27017/scaffold meteor --settings ./settings/settings-development.json",
