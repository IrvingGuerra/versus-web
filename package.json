    "file-type": "^14.1.4",
    "fs-extra": "^9.0.0",
    "jquery": "^3.5.0",
    "meteor-node-stubs": "^1.0.0",
    "node-thumbnail": "^0.15.0",
    "util": "^0.12.2",
    "vee-validate": "2.1.7",
    "vue": "^2.6.11",
    "vue-i18n": "^8.15.5",
    "vue-meteor-tracker": "^2.0.0-beta.5",
    "vue-router": "^3.1.6",
    "vuedraggable": "^2.23.2",
    "vuetify": "^2.2.17",
    "vuetify-image-input": "^19.1.0",
    "vuex": "^3.1.3",
    "vuex-persist": "^2.2.0"
